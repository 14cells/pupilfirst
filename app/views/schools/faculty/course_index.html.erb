<% content_for(:head) do %>
  <title><%= @course.name %> | Coaches</title>
  <%= stylesheet_pack_tag 'SchoolsCoachesCourseIndexPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'SchoolsCoachesCourseIndexPack', nonce: true %>
<% end %>

<% @presenter = Schools::FacultyModule::CourseIndexPresenter.new(self, @course) %>

<div id="sa-coaches-enrollment-panel" class="flex flex-1 h-full" data-props="<%= @presenter.react_props.to_json %>"></div>
