<% content_for(:head) do %>
  <%= javascript_pack_tag 'schools/courses/inactive_students_index', 'data-turbolinks-track': 'reload', nonce: true %>
  <%= stylesheet_pack_tag 'schools/courses/inactive_students_index', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>

<% @presenter = Schools::Founders::IndexPresenter.new(self, @course, inactive: true) %>
<div id="sa-students-panel" class="flex flex-1 h-screen"
     data-props="<%= @presenter.react_props.to_json %>"></div>

