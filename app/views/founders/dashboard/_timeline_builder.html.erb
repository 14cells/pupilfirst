<div class="timeline-builder modal fade">
  <div class="modal-dialog timeline-builder__popup">
    <div class="modal-content timeline-builder__popup-content">
      <div class="timeline-builder__popup-body">
        <button type="button" class="close timeline-builder__modal-close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>

        <%= form_tag timeline_events_path, method: :post, class: 'timeline-builder-hidden-form js-timeline-builder__hidden-form' do |f| %>
        <% end %>

        <%= react_component('TimelineBuilder', { timelineEventTypes: TimelineEventTypes::ListService.new(@startup).list, facebookShareEligibility: current_founder.facebook_share_eligibility } ) %>
      </div>
    </div>
  </div>
</div>
